{
  "name": "titletesterpro-v6",
  "public": true,
  "crons": [
    {
      "path": "/api/cron/rotate-titles",
      "schedule": "0 12 * * *"
    }
  ],
  "functions": {
    "app/api/cron/rotate-titles/route.ts": {
      "maxDuration": 300
    },
    "app/api/cron/reset-quotas/route.ts": {
      "maxDuration": 60
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=()"
        }
      ]
    }
  ]
}